<%- layout("layouts/boilerplate") -%>
<% include ../partials/navbar-alt.ejs %>
<link rel="stylesheet" href="./stylesheets/sermons/index.css">


<% for(let i = 0; i < 3; i++){ %>

    <div class="card mx-5 my-3 shadow bg-light" role="button">
        <div class="row no-gutters">
          <div class="col-md-4">
            <img src="/images/event/bible-study-event.JPG" class="card-img" alt="card image">
          </div>
          <div class="col-md-8">
            <div class="card-body">
              <h5 class="card-title">Card title</h5>
              <p class="card-text">This is a wider card with supporting text below as a natural lead-in to additional content. This content is a little bit longer.</p>
              <p class="card-text"><small class="text-muted">Last updated 3 mins ago</small></p>
            </div>
          </div>
        </div>
    </div>

<% } %>